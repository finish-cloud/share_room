<h1>登録済みルーム一覧</h1>
<thead>
    <tr>
        <th>ルーム画像</th>
        <th>メール名</th>
        <th>ルーム紹介</th>
        <th>料金</th>
        <th>作成日</th>
    </tr>
</thead>

<tbody>
  <% @rooms.each do |room| %>
    <tr>
        <td><%= room.id %></td>
        <td><%= room.image %></td>
        <td><%= room.name %></td>
        <td><%= room.introduction %></td>
        <td><%= room.charge %></td>
        <td><%= room.created_at.to_s(:datetime_jp) %></td>
    </tr>
  <% end %>
</tbody>
